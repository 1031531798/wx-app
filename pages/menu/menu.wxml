<!--pages/menu.wxml-->
<view class="menu-index">
  <view class="menu-header">
    <image class="header-img" src="/src/image/headimg.jfif" mode="aspectFill"></image>
  </view>
  <view class="menu-content">
    <scroll-view class="menu-left">
      <view class="menu-nav {{navIndex === key ? 'menu-nav-hover' : '' }}"  wx:for="{{navList}}" wx:for-index="key" wx:for-item="item" wx:key="key" data-index = '{{key}}' bindtap="clickNav">{{item.label}}</view>
    </scroll-view>
    <scroll-view class="menu-body">
      <text>ssadas</text>
    </scroll-view>
  </view>
</view>
